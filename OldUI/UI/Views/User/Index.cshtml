    @*3196 CFP Header updates added settings-contents class -style="margin-top: 20px;*@
<div class="settings-content page-content">
    <div class="section">
        <div class="section-title">
            <div class="title-wrapper">
                @* @TradeRiserUIResource.PersonalInformation_SectionHeading*@
            </div>
        </div>
        @*<div class="section-title-empty" ></div>*@
        <div class="section-content" id="security-settings">
            <table>
                <thead>
                    <tr>
                        <th>UserName</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Last LogOn Date</th>
                        <th>Locked</th>
                        <th>Disabled</th>
                    </tr>
                </thead>

                <tbody>
                   
                        @foreach (User user in this.Model.Users)
                        {
                        <tr>
                            <td>@user.UserName</td>
                            <td>@user.FirstName</td>
                            <td>@user.LastName</td>
                            <td>@user.Email</td>
                            <td>@user.LastLogOnDate</td>
                            <td>@user.Locked</td>
                            <td>@user.Disabled</td>
                            <td><a href="~/membership/edit/@user.UserName">Edit</a></td>
                        </tr>
                        }
                    
                </tbody>
            </table>
        </div>
    </div>
</div>